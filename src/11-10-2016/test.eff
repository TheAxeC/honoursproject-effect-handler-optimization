
effect EffectExample : unit -> int

let handlerExample = handler
	| #EffectExample () k -> k 5
;;

let f x = x + 5

;;

(*********************************)

(*
NON-optimized example:
    Function call with an addition
*)

with handlerExample handle
    f (#EffectExample ()) + 5

;;

(*********************************)

(*
Optimized example:
    Just a function call is optimized
*)

with handlerExample handle
    f (#EffectExample ())

(*********************************)
